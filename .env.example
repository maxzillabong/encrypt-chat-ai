# Sage Docker Compose Environment Variables
# Copy to .env and fill in your values

# Encryption secret (legacy - ECDH is now preferred)
ENCRYPT_CHAT_SECRET=change-me-in-production

# Claude OAuth token (from Claude CLI: ~/.claude/auth.json)
CLAUDE_CODE_OAUTH_TOKEN=your-oauth-token

# Proxy URL for client
# For production with Caddy: https://sage.maxzilla.nl
NEXT_PUBLIC_PROXY_URL=https://sage.maxzilla.nl
